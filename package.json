{
  "name": "sugarss",
  "version": "0.0.0",
  "description": "Compact alternate syntax for CSS",
  "keywords": [
    "css",
    "postcss",
    "syntax",
    "indent"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "postcss/sugarss",
  "dependencies": {
    "postcss": "^5.0.14"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015-loose":       "7.0.0",
    "eslint-config-postcss":           "1.0.0",
    "babel-preset-stage-0":            "6.3.13",
    "babel-eslint":                    "5.0.0-beta9",
    "babel-core":                      "6.4.5",
    "babel-cli":                       "6.4.5",
    "eslint":                          "1.10.3",
    "ava":                             "0.11.0"
  },
  "scripts": {
    "clean": "rm *.js || echo 'Already cleaned'",
    "build": "npm run clean && babel -d ./ *.es6",
    "test":  "npm run build && ava && eslint *.es6 test/*.js"
  }
}
